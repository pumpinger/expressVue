{
  "name": "expressVue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve-dev": "cross-env NODE_ENV=dev nodemon --watch src/server/**/*.ts  --exec ts-node src/server/app.ts",
    "serve-build": "cd src/server & cross-env NODE_ENV=prod tsc",
    "serve-start": "node build/app.js",
    "serve-pack": "node pack",
    "web-dev": "vite",
    "web-preview": "vite preview",
    "web-build": "vite build",
    "web-build-tsc": "vue-tsc -p src/web/tsconfig.json  --noEmit --skipLibCheck  && vite build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@vueuse/core": "^10.1.2",
    "express": "^4.18.2",
    "pinia": "^2.0.36",
    "typescript": "^5.0.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.1.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2"
  }
